<div id="float-container">
    <img src="https://cdn.pixabay.com/photo/2020/02/10/08/56/emoji-4835770_1280.png" alt="its you :)" id="pfp">
    <div>
        {{!-- spacer --}}
    </div>
    <div id="info">
        <p class="profile">First Name: {{firstNameInput}}</p>
        <p class="profile">Last Name: {{lastNameInput}}</p>
        <p class="profile">Email Address: {{emailAddressInput}}</p>
    </div>
</div>
<p>My Trips: </p>
{{#each trips}}
    <div id="{{this._id}}">
        <p>{{this.name}}</p>
        <p>{{this.start_location}}</p>
        <p>{{this.start_date}}</p>
        <p>{{this.start_time}}</p>
        <p>{{this.end_location}}</p>
        <p>{{this.end_date}}</p>
        <p>{{this.end_time}}</p>
        <p>Stops:</p>
        {{#each this.stops}}
            <p>{{this}}</p>
        {{/each}}
        <p>Things To Do:</p>
        {{#each this.to_do}}
            <p>{{this}}</p>
        {{/each}}
    {{!-- <p>{{this.usersAllowed}}</p> --}}
    </div>

{{/each}}

{{! link to trips }}
{{! link to logout }}
{{! filter trips }}

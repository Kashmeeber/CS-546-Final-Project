<form action = "/trips/map/{{userId}}" method = "POST" id= "mapSearch">
    <label for="newStop">New Stop:
        <input name="newStopInput" id="newStopInput" type="text">
    </label>
    <input type="hidden" name="_method" value="PUT">
    <button type="submit">Submit</button>
</form>
<div id="container">
    <div id="map"></div>
    <div id="sidebar">
    <p>Total Distance: <span id="total"></span></p>
    <div id="panel"></div>
    </div>
</div>
<script
    src="https://maps.googleapis.com/maps/api/js?key=${INSERT_APIKEY_HERE}&callback=Map&v=weekly"
    start="{{mData.start_location}}"
    end="{{mData.end_location}}"
    stops="{{mData.stops}}"
    defer>
</script>
